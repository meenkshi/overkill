@font-face {
    font-family: 'Noto Sans';
    src: url(" {{ 'NotoSans-Regular.woff' | asset_url }}") format('woff'),
        url(" {{ 'NotoSans-Regular.woff2' | asset_url }}") format('woff2');
    font-weight: normal;
    font-style: normal;
    font-display: swap;
}        
/* noto-sans-300 - latin */
@font-face {
    font-display: swap; 
    font-family: 'Noto Sans';
    font-style: normal;
    font-weight: 300;
    src: url(" {{ 'noto-sans-v35-latin-300.woff2' | asset_url }}") format('woff2'); 
}
/* noto-sans-300italic - latin */
